  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class.declaration": {
      "fontStyle": "bold"
    },
    "function.declaration": {
      "fontStyle": "bold"
    },
    "method.declaration": {
      "fontStyle": "bold"
    },
    "interface.declaration": {
      "fontStyle": "bold"
    },
    "type.declaration": {
      "fontStyle": "bold"
    },
    "typeParameter": {
      "foreground": "#00008B",
      "fontStyle": "bold"
    }
  }
